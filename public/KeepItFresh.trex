<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1" 
    xmlns="http://www.tableau.com/xml/extension_manifest">
    <dashboard-extension id="com.theinformationlab.extensions.keepitfresh" extension-version="2.0.0">
        <default-locale>en_US</default-locale>
        <name resource-id="name"/>
        <description>Refresh your data source(s) on a a timer</description>
        <author name="Andre de Vries" email="andre.devries@theinformationlab.co.uk" organization="The Information Lab" website="https://www.theinformationlab.co.uk"/>
        <min-api-version>1.0</min-api-version>
        <source-location>
            <url>https://keepitfresh-extension.theinformationlab.io/</url>
        </source-location>
        <icon></icon>
        <context-menu>
            <configure-context-menu-item />
        </context-menu>
    </dashboard-extension>
    <resources>
        <resource id="name">
            <text locale="en_US">Keep It Fresh</text>
        </resource>
    </resources>
</manifest>
